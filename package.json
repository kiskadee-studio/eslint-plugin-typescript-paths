{
  "name": "eslint-plugin-typescript-paths",
  "version": "0.0.33",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "description": "ESLint Plugin that includes rules which encourage the use of absolute paths over relative paths, defined by paths or baseUrl from tsconfig.json",
  "license": "AGPL-3.0-or-later",
  "author": {
    "email": "rodrigojcmello@gmail.com",
    "name": "Rodrigo JosÃ© Carvalho de Mello",
    "url": "https://github.com/kiskadee-studio"
  },
  "repository": {
    "url": "https://github.com/kiskadee-studio/eslint-plugin-typescript-paths.git"
  },
  "keywords": [
    "alias",
    "typescript",
    "eslint",
    "import",
    "export",
    "absolute",
    "no relative",
    "relative",
    "eslint-plugin",
    "paths",
    "baseurl",
    "tsconfig",
    "module resolution"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "type-check": "tsc",
    "lint": "eslint .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test-ui": "vitest --ui --coverage"
  },
  "dependencies": {
    "jsonc-parser": "^3.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@typescript-eslint/utils": "^5.60.1",
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-kiskadee": "^1.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vitest": "^0.2.6",
    "prettier": "^2.8.8",
    "tsc-alias": "^1.8.6",
    "types-tsconfig": "^2.0.2",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.2"
  },
  "peerDependencies": {
    "eslint": ">=8"
  }
}
